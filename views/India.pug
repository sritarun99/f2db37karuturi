block content
  h1= title
  - var results = results ;
  div(class="container-fluid mt-3")
  div(class="row")
    each temp,index in results
        div(class="col-sm-2" onclick=`buy(${index})`)  #{temp.State1} -  #{temp.Region} - #{temp.State2}
  br
  div(class="container-fluid")
    div(class="row")
    - for(var i=0;i<results.length;i++) {
        div(class="col-sm-2" width="215px" onClick=`buy(${i+1})`) #{results[i].State1}
    -}
  script. 
    function buy(indexNum) {
      alert(`You purchased item ${indexNum}`);
    }