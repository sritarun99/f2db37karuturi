block content
  h1= title
  - var results = results ;
  div(class="container-fluid mt-3")
  div(class="row m-1")
    each temp,index in results
        - let destination = `dispatch(\"/india/update?id=${temp._id}\")` 
        div(class="col-sm-2", class="m-1")  #{temp.State1} -  #{temp.Region} - #{temp.State2}
          button.btn( class="btn-primary ml-1", onclick= destination) Update
  br
  div(class="container-fluid")
    div(class="row")
    - for(var i=0;i<results.length;i++) {
        div(class="col-sm-2" width="215px" onClick=`buy(${i+1})`) #{results[i].State1}
    -}
  script. 
    function buy(indexNum) {
      alert(`You purchased item ${indexNum}`);
    }
    function dispatch(path){ 
      window.location.href=path 
    } 